        var contentSpot = (function () {
          var countryCode = 'countryCode=US';
          var divisionCode = 'S&amp;T';
          var params = [
            'region=US',
            'division=' + encodeURIComponent(divisionCode.replace(/&amp;/g, '&')),
            'type=Books'
          ];
      
            params.push('isbn=978-0-12-381479-1');
      
      
          function createMarketingSpots(spots) {
            document.getElementById('marketing-spots').innerHTML = '';
            if (spots && spots.length) {
              for (var i = 0; i < spots.length; i++) {
                var container = document.createElement('div');
                container.setAttribute('class', 'marketing-spot');
                var img = document.createElement('img');
                img.setAttribute('src', spots[i].large_img_url);
      
                if (spots[i].spot_url) {
                  var anchor = document.createElement('a');
                  anchor.setAttribute('href', spots[i].spot_url);
                  anchor.setAttribute('target', '_blank');
                  anchor.appendChild(img);
                  container.appendChild(anchor);
                } else {
                  container.appendChild(img);
                }
      
                document.getElementById('marketing-spots').appendChild(container);
              }
            }
          }
      
          function loadContentSpots(changedCountryCode) {
            var queryString;
      
            if (changedCountryCode) {
              queryString = 'countryCode=' + changedCountryCode + '&' + params.join('&');
            } else {
              queryString = countryCode + '&' + params.join('&');
            }
      
            $.ajax({
              url: '/_rest_magic/content-spot/_nocache?' + queryString,
              cache: false,
              success: createMarketingSpots,
              error: function (xhr, status, error) {
                if (error) {
                  $('.marketing-spots').hide();
                }
              }
            });
          }
      
          return {
            load: loadContentSpots
          }
        })();
      
        $(function () {
          contentSpot.load();
