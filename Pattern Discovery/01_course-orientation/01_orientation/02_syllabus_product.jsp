    if ($this.attr('id') !== 'info-country-selector') {
        if (!functionalCookiesEnabled()) {
            window.location.href = getQueryString('countrycode')
                ? removeQueryParameterFrom(window.location.href, 'countrycode') + 'countrycode=' + countryCode
                : window.location.href + '?countrycode=' + countryCode;
            return;
        }
        window.location.href = getQueryString('countrycode')
            ? removeQueryParameterFrom(window.location.href, 'countrycode')
            : window.location.href;
        return;
