  function functionalCookiesEnabled() {
      // Cookie opt-in groups: Necessary (1), Performance (2), Functional (3), Targeting (4)
      // This cookie has been deemed "Functional"
      // window.OptanonActiveGroups is a string with opted in groups separated by ","
      // If the user has opted into all cookies, it will be ",1,3,2,4,"
      // If that string doesn't contain "3" then we just do nothing.
      return window.location.host.indexOf('localhost') !== -1
          || (window.OptanonActiveGroups && window.OptanonActiveGroups.indexOf('3') !== -1);
  }

  function saveLocationCookie(countryCode) {
      if (!functionalCookiesEnabled()) {
        return;
      }
      // 4 weeks
      var d = new Date();
      d.setTime(d.getTime() + 2419200000);
      document.cookie = 'cloc='+countryCode+';expires='+d.toUTCString()+';path=/;';
  }

  function showPrice(countryCode) {
    var currencyCode = currencies[countryCode] ? currencies[countryCode].code : '';

    // Reset prices and button back to default
    $('[data-currency],.format-price-unavailable').addClass('hide');
    $('.format-variations').removeClass('hide');
    $('.add-button').removeClass('disabled').removeAttr('disabled');

    // Show the correct price for the selected country
    $('[data-currency="'+currencyCode+'"][data-region="'+countryCode+'"]:not(.slash-price)').removeClass('hide');
    if(!$('[data-currency]:visible:not(.slash-price)').length) {
      // If no price matches currency and country, fall back to the world version for the currency
      $('[data-currency="'+currencyCode+'"][data-region="WORLD"]:not(.slash-price)').removeClass('hide');
      if(!$('[data-currency]:visible:not(.slash-price)').length) {
        // Finally, if no matching currency is found, fall back to the price without a region
        $('[data-currency="'+currencyCode+'"][data-region=""]:not(.slash-price)').removeClass('hide');
        if(!$('[data-currency]:visible:not(.slash-price)').length) {
          // If we can't find any prices we show as unavailable
          showUnavailable();
        }
      }
    }

    // Show slash prices
    $('.slash-price[data-currency="'+currencyCode+'"][data-region="'+countryCode+'"]').removeClass('hide');
    if(!$('.slash-price[data-currency]:visible').length) {
      var regionCode = currencies[countryCode] ? currencies[countryCode].region : 'US';
      $('.slash-price[data-currency="'+currencyCode+'"][data-region="'+regionCode+'"]').removeClass('hide');
      if(!$('.slash-price[data-currency]:visible').length) {

        // If no price matches currency and country, fall back to the world version for the currency
        $('.slash-price[data-currency="'+currencyCode+'"][data-region="WORLD"]').removeClass('hide');
        if(!$('.slash-price[data-currency]:visible').length) {
          // Finally, if no matching currency is found, fall back to the price without a region
          $('.slash-price[data-currency="'+currencyCode+'"][data-region="all"]').removeClass('hide');
          if(!$('.slash-price[data-currency]:visible').length) {
            // No slash prices
          }
        }
      }
    }
    addTax(countryCode);
  }

  function addTax(countryCode) {
    var buyingOptions = $('[data-currency]:visible');
    var isArgi = $('div.is-argi').data('is-argi') || false;

    if (isArgi) {
      countryCode = $('.country-selector').find(':selected').data('country-code');
    }

    buyingOptions.each(function() {
      var taxRates = JSON.parse($(this).parent('.format-prices').find('.tax-rates').val() || '{}');
      var taxRate = taxRates[countryCode];
      var price = parseFloat($(this).data('price'));
      if (taxRate) {
        price = price + price * (taxRate / 100);
        $(this).find('.price-value').text(price.toFixed(2));
      } else {
        $(this).find('.price-value').text(price.toFixed(2));
      }
      if (taxRate === undefined) {
        $('.tax-included-message').addClass('hide');
        $('.tax-excluded-message').removeClass('hide');
      } else {
        $('.tax-included-message').removeClass('hide');
        $('.tax-excluded-message').addClass('hide');
        $(this).parent('.format-prices').siblings('.tax-rate-message').find('.tax-rate-message-amount').text('of ' + taxRate + '%');
      }

      if (isArgi) {
        if (taxRate !== undefined) {
          $('.tax-included-message').addClass('hide');
          $('.tax-excluded-message').addClass('hide');
        } else {
          $('.tax-included-message').addClass('hide');
          $('.tax-excluded-message').removeClass('hide');
        }
      }
    });
  }

  function setHiddenCountryCode(countrySelector) {

    // If there is a hidden country code within the same form as countrySelector then make sure
    // it contains the country code held in the data property. If that is not populated
    // use the selected value instead. This ensures that promotions
    // work for argi journals whose country code (for pricing) would be WORLD. It also covers
    // any other products where the country-code data property is not populated.
    const countryCode = countrySelector.closest('form').find('.submit-country');
