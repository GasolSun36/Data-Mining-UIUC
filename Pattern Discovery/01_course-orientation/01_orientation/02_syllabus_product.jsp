  function getQueryString(name) {
    var results = new RegExp('[\?&]' + name + '=([^&#]*)')
                      .exec(window.location.search);

    return (results !== null) ? results[1] || 0 : false;
  }

  function removeQueryParameterFrom(originalUrl, qparameter) {
    let url = originalUrl;
    const parts = originalUrl.split('?');
    url = parts[0];
    if (parts.length > 0) {
        let qs = '';
        const qsArgs = parts[1].split('&');
        qsArgs.forEach(function(arg) {
            if (arg.indexOf(qparameter) <0 ) {
                if (qs.length > 0) {
                    qs += '&';
                }
                qs += arg;
            }
        })
        url = url + '?' + qs;
    }
    return url;
  }


